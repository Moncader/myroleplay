<Ui xmlns="http://www.blizzard.com/wow/ui/"   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"   xsi:schemaLocation="http://www.blizzard.com/wow/ui/"> 
	<Script file="MyTime.lua" />

	<Frame name="mtiTimeWatcher" parent="UIParent" hidden="false">
		<Scripts>
			<OnLoad>
				mtiOnLoad();
			</OnLoad>
			<OnUpdate>
				mtiOnUpdate(arg1);
				mtiUpdateTimers(arg1);
			</OnUpdate>
			<OnEvent>
				mtiOnEvent(event, arg1, arg2, arg3, arg4, arg5, arg6, arg7, arg8, arg9);
			</OnEvent>
		</Scripts>		
	</Frame>
</Ui>